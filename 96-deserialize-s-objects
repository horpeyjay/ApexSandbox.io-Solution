public List<Account> getAccountsFromJSONString(String inputJSON){
    // code here
    List<Account> accounts = new List<Account>();
    List<Object> stringFromJson = JSON.deserializeUntyped(inputJSON);

    for(Object obj : stringFromJson){
        Map<String, Object> objectMap = (Map<String, Object>) obj;
        Account acc = new Account();
        acc.Id = (Id) objectMap.get('Id');
        acc.Name = (String) objectMap.get('Name');

        accounts.add(acc);
    }
    return accounts;
}