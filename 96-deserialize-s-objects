public List<Account> getAccountsFromJSONString(String inputJSON){
    // code here
    List<Account> accounts = new List<Account>();
    Map<String, List<Account>> stringFromJson = JSON.deserializeUntyped(inputJSON);

    for(Account a : stringFromJson.values()){
        Account acc = new Account();
        acc.Id = (Id) stringFromJson.get('Id');
        acc.Name = (String) stringFromJson.get('Name');

        accounts.add(acc);
    }
    return accounts;
}