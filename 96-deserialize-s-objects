public List<Account> getAccountsFromJSONString(String inputJSON){
    // code here
    List<Account> accounts = new List<Account>();
    try{
        List<Object> stringToJson = (List<Object>) JSON.deserializeUntyped(inputJSON);

        for(Object o : stringToJson){
            Map<String, Object> accountMap = (Map<String, Object>) o;

            Account acc = new Account();

            if(accountMap.containsKey('attributes')){
                Map<String, Object> attributes = (Map<String, Object>) accountMap.get('attributes');

                if(attributes.containsKey('Id')){
                    acc.Id = (Id) attributes.get('Id');
                }

                if(attributes.containsKey('Name')){
                    acc.Name = attributes.get('Name');
                }
                accounts.add(acc);
            }
        }
    }catch (Exception ex){
        system.debug(ex.getMessage());
        return null;
    }

    return accounts;
}