public List<Account> getAccountsFromJSONString(String inputJSON){
    // code here
    List<Account> accounts = new List<Account>();
    Map<String, Object> stringFromJson = (Map<String, Object>) JSON.deserializeUntyped(inputJSON);
    Map<String, String> attributes = (Map<String, String>) stringFromJson.get('attributes');
    for(String str : attributes.values()){
        
        Account acc = new Account();
        acc.Id = (Id) attributes.get('Id');
        acc.Name = (String) attributes.get('Name');

        accounts.add(acc);
    }
    return accounts;
}