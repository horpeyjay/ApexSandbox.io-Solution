public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> caseTypeMap = new Map<String, List<Case>>();
    if(!cases.isEmpty()){
        for(Case c : cases){
            if(c.Type != null){
                List<Case> caseType = new List<Case>();
                caseType.add(c);
                if(!caseTypeMap.containsKey(c.Type)){
                    caseTypeMap.put(c.Type, caseType);
                }else{
                    List<Case> caseType = caseTypeMap.get(c.Type).add(c);
                    caseTypeMap.put(c.Type, caseType);
                }
                
            }
        }
    }
    return caseTypeMap;
}