public List<Account> getAccounts(List<Account> accounts) {
    // code here
    List<AccountWrap> accountWrap = new List<AccountWrap>();

    for(Account a : accounts){
        accountWrap.add(new AccountWrap(a));
    }

    accountWrap.sort();

    List<Account> newList = new List<Account>();

    for(AccountWrap aw : accountWrap){
        newList.add(aw.acc);
    }
    return newList;
}
// write extra class here 

public class AccountWrap implements Comparable{
    public Account acc;

    public AccountWrap (Account acc){
        this.acc = acc;
    }

    public Integer compareTo(Object other) {
        AccountWrap aw = (AccountWrap) other;
        Integer prevAcc = (aw.acc.AnnualRevenue != null) ? Integer.valueOf(aw.acc.AnnualRevenue) : 0;
        Integer nextAcc = (this.acc.AnnualRevenue != null) ? Integer.valueOf(this.acc.AnnualRevenue) : 0;

        // Compare in descending order
        return prevAcc > nextAcc ? -1 : (prevAcc < nextAcc ? 1 : 0);
    }

}

