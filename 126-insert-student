public Id insertStudent(String name, String email) {
    // code here
    List<apxio__Student__c> aSC = new List<apxio__Student__c>();
    if(name != null && email != null){
        aSC.Name = name;
        aSC.apxio__Email__c = email;
    }
    insert aSC;
    Id studentId = aSC.Id;
    return studentId;
}