public Decimal average(List<Decimal> numbers){
    // your code here
    Decimal mean;
    Decimal addedNumbers = 0;
    if(!numbers.isEmpty() || numbers != null){
        for(Integer i = 0; i < numbers.size(); i++){
           
            addedNumbers += numbers[i];
            
        }

    }
    mean = (addedNumbers / numbers.size()).setScale(2);
    return mean;
}