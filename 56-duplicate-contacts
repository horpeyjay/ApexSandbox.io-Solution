public Boolean duplicateContacts(Contact c1, Contact c2) {
    //code here
    try{
        if(c1.Email != null && c1.Phone != null && c2.Email != null && c2.Phone != null){
            if(c1.Phone != c2.Phone || c1.Email != c2.Email){
                return true;
            }
            return false;
        }
    }catch(Exception ex){
        return false;
    }
    return false;
}