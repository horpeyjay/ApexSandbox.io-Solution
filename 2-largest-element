public static Integer findLargest(List<Integer> nums) {
	//code here
    Integer greatest;
    if(!nums.isEmpty()){
        for(Integer i = 0; i < nums.size(); i++){
            if(i == 0){
                greatest = nums[0];
            }
            if(nums[i] > greatest){
                greatest = nums[i];
            }
        }
    }
    return greatest;
}