public static List<Integer> twoSum(List<Integer> nums, Integer target) {
    //code here
    List<Integer> output = new List<Integer>();
    for(Integer i = 0; i < nums.size(); i++){
        Integer othernum = 0;
        if(nums[i] < target){
            output.add(i);
            othernum = target - i;
        }

        if(nums[i] == othernum){
            output.add(i);
        }
    }

    return output;

}