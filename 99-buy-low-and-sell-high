public Integer maxProfit(List<Integer> prices){
    //code here
    Integer profit = 0;
    Integer smallPrice;
    if(!prices.isEmpty()){
        for(Integer i = 0; i < prices.size(); i++){
            if(i == 0){
                smallPrice = prices[0];
            }

            if(prices[i] < smallPrice){
                Integer holdingProfit = smallPrice - prices[i];

                smallPrice = prices[i];

                if(holdingProfit > profit){
                    profit = holdingProfit;
                }
            }
        }
    }

    return profit;
}