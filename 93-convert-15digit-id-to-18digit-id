public String convert15to18DigitId(String fifteenDigit)
{
    //code here
    string error = '-1';
    String success;
    Id Id18new;
    if(fifteenDigit.length() != 15){
        return error;
    }else{
        if(fifteenDigit != null){
            Id18new = Id.valueOf(fifteenDigit);
            
        }
    }
    return Id18new;
}