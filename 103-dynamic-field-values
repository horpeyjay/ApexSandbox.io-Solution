public List<String> getFieldsValue(Account acc, List<String> fields)
{
    // code here
    List<String> result = new List<String>();
    for (String s : fields){
        Object value = acc.get(s);
        if(value != null){
            String field = String.valueOf(acc.get(value));
            result.add(field);
        }else{
            result.add(null);
        }
    }

    return result;

}