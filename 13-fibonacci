public Integer fibonacci(Integer n) {
    //code here
    if (n == 1 or n ==2 ){
        return 1
    }

    Integer previousValue = 1;
    Integer nextValue = 1;

    for(Integer i = 3; i <= n; i++){
        Integer temp = previousValue + nextValue;
        nextValue = previousValue;
        previousValue = temp;
    }

    return nextValue;
}