public Integer nextPrime(Integer num) {    //code here
    Integer primeNumber = num + 1;
    while (true){
        if(isPrime(primeNumber)){
            return primeNumber;
        }else{
            primeNumber++;
        }
    }
    return primeNumber;

    
}

public static Boolean isPrime(Integer num){
    
    for(Integer i = 2; i < Math.sqrt(num); i++){
        if(Math.mod(num, i) == 0){
            return false;
        }
    }
    return true;
}