public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {
    //code here
    
    if(nums1 == null && nums2 == null){
        return true;
    }

    if(nums1 == null || nums2 == null){
        return false;
    }
    
    List<Integer> sort1 = new List<Integer>();
    List<Integer> sort2 = new List<Integer>();

    sort1.addAll(nums1);
    sort2.addAll(nums2);

    return sort1.equals(sort2);
}