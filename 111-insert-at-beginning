public void insertAtStart(String cityName, List<String> cities) {
    //code here
    try{
    if(!String.isBlank(cityName) || cityName != ''){
        if(cities == null || cities.isEmpty()){
            cities = new List<String>();
            cities.add(cityName);
        }else{
            cities.add(0, cityName);
        }
    }
    }catch(Exception ex){
        system.debug(ex.getMessage());
    }
}