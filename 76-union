public Set<Integer> setUnion(Set<Integer> set1, Set<Integer> set2) {
    //code here
    Set<Integer> newSet = new Set<Integer>();
    if(set1.size() > 0){
        for(Integer i : set1){
            newSet.add(i);
        }
    }
    if(set2.size() > 0){
        for(Integer j : set2){
            newSet.add(j);
        }
    }

    return newSet;
}